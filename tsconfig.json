{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist",
		"rootDir": "api",
		"allowImportingTsExtensions": true
	},
	"include": ["api"],
	"exclude": ["node_modules"]
}
